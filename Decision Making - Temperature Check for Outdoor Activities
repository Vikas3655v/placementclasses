import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int t = in.nextInt();
        if (t>0){
            System.out.print("Safe for outdoor activities");
        }
        else{
            System.out.print("Too cold for outdoor activities");
            
        }
    }
}
