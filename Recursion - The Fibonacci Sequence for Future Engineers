import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int a = in.nextInt();
        int result = fib(a);
        System.out.print(result);
        }
        public static int fib(int a){
            if(a==0){
                return 0;
            }
            else if(a==1){
                return 1;
            }
            else{
                return fib(a-1)+fib(a-2);
            }
        }
    }
